{
  "name": "mesour/datagrid",
  "description": "Mesour DataGrid is DataGrid for PHP >= 5.5 with options like to inline edit, export to csv, create sub items, sort data using jQuery.ui.nestedSortable and much more.",
  "license": ["BSD-3-Clause","GPL-3.0"],
  "keywords": ["grid","datagrid","data grid","component"],
  "homepage": "http://mesour.com",
  "authors": [{
      "name": "Matouš Němec",
      "homepage": "http://mesour.com"
  }],
  "require": {
    "php": ">=5.5.0",
    "mesour/filter": "~0.1.0-beta",
    "mesour/components": "~0.1.0-beta",
    "mesour/selection": "~0.1.0-beta",
    "mesour/dropdown": "~0.1.0-beta",
    "mesour/button": "~0.1.0-beta",
    "mesour/pager": "~0.1.0-beta",
    "mesour/icon": "~0.1.0-beta",
    "mesour/table": "~0.1.0-beta"
  },
  "require-dev": {
    "tracy/tracy": "~2.3.0",
    "latte/latte": "~2.3.0",
    "nette/robot-loader": "~2.3.0",
    "nette/database": "~2.3.0",
    "doctrine/orm": "^2.5",
    "mesour/array-manager": "dev-master"
  },
  "minimum-stability": "beta",
  "prefer-stable": true,
  "suggest": {
    "latte/latte": "to use column Template or Template SubItem"
  },
  "autoload": {
    "classmap": ["src/"]
  }
}

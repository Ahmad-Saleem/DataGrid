{
  "name": "mesour/datagrid",
  "description": "Mesour DataGrid is DataGrid for PHP >= 5.5 with options like to inline edit, export to csv, create sub items, sort data using jQuery.ui.nestedSortable and much more.",
  "license": ["BSD-3-Clause","GPL-2.0","GPL-3.0"],
  "keywords": ["grid","datagrid","data grid","component"],
  "homepage": "http://mesour.com",
  "authors": [{
      "name": "Matouš Němec",
      "homepage": "http://mesour.com"
  }],
  "require": {
    "php": ">=5.5.0",
    "mesour/sources": "@dev",
    "mesour/filter": "@dev",
    "mesour/components": "@dev",
    "mesour/selection": "@dev",
    "mesour/dropdown": "@dev",
    "mesour/button": "@dev",
    "mesour/pager": "@dev",
    "mesour/icon": "@dev",
    "mesour/table": "@dev"
  },
  "require-dev": {
    "tracy/tracy": "~2.3.0",
    "latte/latte": "~2.3.0",
    "nette/robot-loader": "~2.3.0",
    "nette/database": "~2.3.0",
    "doctrine/orm": "^2.5",
    "mesour/array-manager": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "suggest": {
    "latte/latte": "to use column Template or Template SubItem",
    "ext-gd": "to use column Image"
  },
  "autoload": {
    "classmap": ["src/"]
  }
}
